{
  "name": "webpage-change-tracker",
  "version": "1.0.0",
  "description": "track if a webpage has changed since the first time you checked it",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node ./api/api.js",
    "client": "cd ./frontend && npm start",
    "install-server": "npm install",
    "install-client": "cd ./frontend && npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwuzy/webpage-change-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kwuzy/webpage-change-tracker/issues"
  },
  "homepage": "https://github.com/kwuzy/webpage-change-tracker#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cra-template": "1.2.0",
    "crypto": "^1.0.1",
    "express": "^4.21.2",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
